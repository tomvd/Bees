<?xml version="1.0" encoding="utf-8"?>
<Defs>
      <!-- ============= TakeHoneyOutOfBeehive ============= -->

      <WorkGiverDef>
        <defName>Bees_TakeHoneyOutOfBeehive</defName>
        <label>take honey out of Beehive</label>
        <giverClass>Bees.WorkGiver_TakeHoneyOutOfBeehive</giverClass>
        <workType>Handling</workType>
        <verb>take honey</verb>
        <gerund>taking honey from</gerund>
        <priorityInType>20</priorityInType>
        <requiredCapacities>
            <li>Manipulation</li>
        </requiredCapacities>
    </WorkGiverDef>


  <JobDef>
    <defName>Bees_TakeHoneyOutOfBeehive</defName>
    <driverClass>Bees.JobDriver_TakeHoneyOutOfBeehive</driverClass>
    <reportString>taking honey out of TargetA.</reportString>
    <allowOpportunisticPrefix>true</allowOpportunisticPrefix>
  </JobDef>

